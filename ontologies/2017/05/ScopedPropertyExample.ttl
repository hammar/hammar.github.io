# baseURI: http://www.karlhammar.com/ontologies/2017/05/ScopedPropertyExample

@prefix : <http://www.karlhammar.com/ontologies/2017/05/ScopedPropertyExample#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix spin: <http://spinrdf.org/spin#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.karlhammar.com/ontologies/2017/05/ScopedPropertyExample>
  rdf:type owl:Ontology ;
  spin:imports <http://topbraid.org/spin/owlrl-all> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
:AudiA6
  rdf:type :Car ;
  rdfs:label "Audi a6" ;
.
:BMW_Superbike
  rdf:type :Motorcycle ;
  :ownedBy :Karl ;
  rdfs:label "BMW Superbike" ;
.
:Car
  rdf:type owl:Class ;
  rdfs:label "Car" ;
  rdfs:subClassOf :Vehicle ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty :ownedBy ;
      owl:someValuesFrom :CarOwner ;
    ] ;
.
:CarOwner
  rdf:type owl:Class ;
  rdfs:label "Car owner" ;
  rdfs:subClassOf :Person ;
  owl:equivalentClass [
      rdf:type owl:Restriction ;
      owl:onProperty :owns ;
      owl:someValuesFrom :Car ;
    ] ;
.
:Karl
  rdf:type :Person ;
  :owns :AudiA6 ;
  rdfs:label "Karl" ;
.
:Motorcycle
  rdf:type owl:Class ;
  rdfs:label "Motorcycle" ;
  rdfs:subClassOf :Vehicle ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty :ownedBy ;
      owl:someValuesFrom :MotorcycleOwner ;
    ] ;
.
:MotorcycleOwner
  rdf:type owl:Class ;
  rdfs:label "Motorcycle owner" ;
  rdfs:subClassOf :Person ;
  owl:equivalentClass [
      rdf:type owl:Restriction ;
      owl:onProperty :owns ;
      owl:someValuesFrom :Motorcycle ;
    ] ;
.
:Person
  rdf:type owl:Class ;
  rdfs:label "Person" ;
  rdfs:subClassOf owl:Thing ;
.
:Vehicle
  rdf:type owl:Class ;
  rdfs:label "Vehicle" ;
  rdfs:subClassOf owl:Thing ;
.
:ownedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Vehicle ;
  rdfs:label "owned by" ;
  rdfs:range :Person ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  owl:inverseOf :owns ;
.
:owns
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Person ;
  rdfs:label "owns" ;
  rdfs:range :Vehicle ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
